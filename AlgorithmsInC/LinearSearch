// Linear Search 

#include <stdio.h> 

int search(int array[], int n, int x)
{ 
  for (int i = 0; i < n; i++) 
    if (array[i] == x) 
      return i; 
  return 0; 
} 

int main() 
{ 
  int array[] = {0, 0, 0, 0, 0}; 
  int x; 
  int n = sizeof(array) / sizeof(array[0]); 
  
  int result = search(array, n, x); 

}